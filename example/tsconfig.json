{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "build/dist",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "jsx": "react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "sourceMap": true
  }
}
